geom_bar(stat = "identity", width = 0.8) +
labs(x = "Rango de Velocidad de subida (Mbps)", y = "Frecuencia", title = "Frecuencia de Velocidad de Subida") +
theme(axis.text.x = element_text(angle = 45, hjust = 1)) +
coord_flip()
